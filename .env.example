# Supabase - Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Resend - Get this from https://resend.com/api-keys
RESEND_API_KEY=re_your_api_key

# App URL - Set to your domain in production
NEXT_PUBLIC_APP_URL=http://localhost:3000
# For production (Vercel): NEXT_PUBLIC_APP_URL=https://instapi.app

# Google Analytics (optional) - Get from https://analytics.google.com
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
